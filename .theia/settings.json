{
  "editor.autoIndent": true,
  "editor.tabSize": 4,
  "editor.dragAndDrop": true,
  "editor.renderIndentGuides": true,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.autoSave": "on",
